{
  "name": "intl-datetimeformat-options",
  "version": "1.0.1",
  "description": "Provides localized date/time format patterns for styles full, long, medium and short, using Intl.DateTimeFormat.",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/intl-datetimeformat-options/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/intl-datetimeformat-options#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/intl-datetimeformat-options.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/intl-datetimeformat-options/issues"
  },
  "engines": {
    "node": ">=20.10"
  },
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "browser": "lib/index.min.js",
  "types": "lib/index.d.ts",
  "exports": {
    "require": "./lib/index.cjs",
    "import": "./lib/index.js",
    "browser": "./lib/index.min.js",
    "types": "./lib/index.d.ts"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prepare": "rollup -c",
    "lint": "biome lint *.js lib test util && tsc --noEmit test/types.test.ts",
    "check": "teru-cjs test/cjs.test.cjs && teru-esm test/*.js",
    "cover": "c8 teru-esm test/*.js",
    "test": "biome lint *.js lib test util && tsc --noEmit test/types.test.ts && teru-cjs test/cjs.test.cjs && c8 teru-esm test/*.js"
  },
  "c8": {
    "check-coverage": "true",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "c8": "10.1.3",
    "cldr-core": "47.0.0",
    "cldr-dates-full": "47.0.0",
    "cldr-misc-full": "47.0.0",
    "cldr-numbers-full": "47.0.0",
    "rollup": "4.46.2",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-swc-minify": "1.3.0",
    "tehanu": "1.0.1",
    "tehanu-repo-coco": "1.0.1",
    "tehanu-teru": "1.0.1",
    "typescript": "5.9.2"
  },
  "keywords": [
    "intl",
    "date",
    "time",
    "datetime",
    "locale",
    "pattern",
    "format",
    "cldr",
    "unicode",
    "ldml"
  ]
}
